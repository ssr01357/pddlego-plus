    "def summarize_obs(obs):\n",
    "    # If obs only has one line, return it\n",
    "    if len(obs.split('\\n')) == 1:\n",
    "        return obs\n",
    "    # Only keep where you are and location informtion\n",
    "    else:\n",
    "        return obs.split('\\n')[0].split(\". \")[0] + \". \" + obs.split('\\n')[1]"
   ]
